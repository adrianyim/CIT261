{
  "name": "parcel-demo",
  "version": "1.0.0",
  "description": "Boilerplate for a simple parcel based web app",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf build/*",
    "start": "parcel src/index.html -d build/",
    "prebuild": "rimraf dist/*",
    "build": "parcel build src/index.html --public-url ./",
    "watch": "parcel watch src/index.html -d build/",
    "lint": "eslint *.js src/**/*.js"
  },
  "author": "Shane T",
  "license": "GPL-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.8.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.9.4",
    "parcel-bundler": "^1.10.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {},
  "resolutions": {
    "browserslist": "^3.0.0"
  }
}
